{
  "name": "svjs",
  "version": "1.0.2",
  "description": "Scalable Vector JavaScript. A very light object-oriented library to help generative artists working with SVG.",
  "keywords": [
    "creative coding",
    "generative art",
    "javascript svg",
    "scripting svg",
    "code art",
    "svgjs",
    "svjs",
    "svg"
  ],
  "author": "David Matthew <davidmatthew.ie@gmail.com> (https://davidmatthew.ie)",
  "license": "ISC",
  "source": "src/index.ts",
  "module": "dist/svjs.min.js",
  "types": "dist/index.d.ts",
  "targets": {
    "module": {
      "optimize": true
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build:types": "tsc",
    "build": "npm run clean && parcel build && npm run build:types",
    "start": "parcel watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidmatthew-ie/svjs.git"
  },
  "bugs": {
    "url": "https://github.com/davidmatthew-ie/svjs/issues"
  },
  "homepage": "https://github.com/davidmatthew-ie/svjs#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.8.2",
    "parcel": "^2.8.2",
    "typescript": "^5.9.3"
  }
}
